{
  "name": "nuxt3-biolerplate",
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "dev": "concurrently \"yarn tw-config-serve\" \"nuxi dev\"",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "postinstall": "nuxi prepare",
    "tw-config-serve": "serve -l 3001 ./.tw-preview",
    "tw-config-out": "rm -rf ./.tw-preview && tailwind-config-viewer export ./.tw-preview"
  },
  "devDependencies": {
    "@headlessui/vue": "1.6.7",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "0.5.4",
    "@antfu/eslint-config": "^0.25.2",
    "@iconify-json/ph": "1.1.2",
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/twemoji": "^1.1.5",
    "@nuxtjs/color-mode": "^3.1.4",
    "@pinia/nuxt": "^0.3.0",
    "@unocss/nuxt": "^0.44.5",
    "@vueuse/nuxt": "^8.9.4",
    "eslint": "^8.20.0",
    "nuxt": "^3.0.0-rc.6",
    "pinia": "^2.0.16",
    "typescript": "^4.7.4",
    "@nuxtjs/tailwindcss": "5.3.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "@intlify/unplugin-vue-i18n": "^0.2.1",
    "vue-i18n": "^9.1.10",
    "concurrently": "7.3.0",
    "serve":"^14.0.1",
    "sass": "1.54.0",
    "sass-loader": "13.0.2"
  }
}
